@model IList<ProductOverviewModel>
@if (Model.Count > 0)
{
    <div class="bs_home_feature_product_section">
        <div class="product-grid home-page-product-grid bs_home_feature_product_main">
            <div class="bs_title_white">
                <h3> @T("Homepage.Products")</h3>
            </div>
            <div class="item-grid  bs_home_products">
                @foreach (var item in Model)
                {
                    <div class="item-box">
                        @await Html.PartialAsync("_ProductBox", item)
                    </div>
                }
            </div>
        </div>
    </div>
}